{
    "run_type": "staged_learning",
    "device": "cpu",
    "use_cuda": true,
    "tb_logdir": "tb_logs",
    "json_out_config": "gen_mols.json",
    "seed": null,
    "parameters": {
        "prior_file": "../priors/mol2mol_medium_similarity.prior",
        "agent_file": "../priors/mol2mol_medium_similarity.prior",
        "summary_csv_prefix": "gen_mols",
        "use_checkpoint": false,
        "purge_memories": false,
        "smiles_file": "despigment_actives.smi",
        "sample_strategy": "multinomial",
        "distance_threshold": 100,
        "batch_size": 8,
        "randomize_smiles": true,
        "unique_sequences": true,
        "temperature": 1.0,
        "tb_isim": false
    },
    "scoring": null,
    "scheduler": null,
    "responder": null,
    "filter": null,
    "stage": [
        {
            "max_steps": 100,
            "max_score": 0.8,
            "chkpt_file": "mol2mol_stage1.chkpt",
            "termination": "simple",
            "min_steps": 10,
            "scoring": {
                "type": "geometric_mean",
                "component": [
                    {
                        "TanimotoDistance": {
                            "endpoint": [
                                {
                                    "name": "Tanimoto vs seeds",
                                    "weight": 0.7,
                                    "params": {
                                        "smiles": [
                                            "Oc1ccccc1O",
                                            "Oc1ccc(O)cc1",
                                            "Oc1cccc(O)c1",
                                            "O=C(O)c1ccccc1O"
                                        ],
                                        "radius": 2,
                                        "use_counts": true,
                                        "use_features": false
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "QED": {
                            "endpoint": [
                                {
                                    "name": "QED",
                                    "weight": 0.6
                                }
                            ]
                        }
                    },
                    {
                        "SAScore": {
                            "endpoint": [
                                {
                                    "name": "SA score",
                                    "weight": 0.3
                                }
                            ]
                        }
                    }
                ],
                "filename": null
            },
            "diversity_filter": null
        }
    ],
    "learning_strategy": {
        "type": "dap",
        "sigma": 80,
        "rate": 0.0001
    },
    "diversity_filter": {
        "type": "IdenticalMurckoScaffold",
        "bucket_size": 25,
        "minscore": 0.5,
        "minsimilarity": 0.5,
        "penalty_multiplier": 0.5
    },
    "inception": null
}